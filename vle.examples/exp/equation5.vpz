</out>
</model>
</submodels>
<connections>
<connection type="internal">
 <origin port="update" model="a"/>
 <destination port="update" model="b"/>
</connection>
<connection type="internal">
 <origin port="update" model="a"/>
 <destination port="update" model="d"/>
</connection>
<connection type="internal">
 <origin port="update" model="b"/>
 <destination port="update" model="c"/>
</connection>
<connection type="internal">
 <origin port="update" model="c"/>
 <destination port="update" model="d"/>
</connection>
<connection type="internal">
 <origin port="a" model="perturb"/>
 <destination port="perturb" model="a"/>
</connection>
</connections>
</model>
</structures>
 <dynamics>
<dynamic name="dynA" package="vle.examples" library="EquationA1" type="local"/>
<dynamic name="dynB" package="vle.examples" library="EquationB1" type="local"/>
<dynamic name="dynC" package="vle.examples" library="EquationC1" type="local"/>
<dynamic name="dynD" package="vle.examples" library="EquationD1" type="local"/>
<dynamic name="dynPerturb" package="vle.examples" library="EquationPerturb1" type="local"/>
</dynamics>
 <experiment name="exp" combination="linear">
<conditions>
<condition name="simulation_engine">
 <port name="duration">
<double>20.0</double>
</port>
 <port name="begin">
<double>0.0</double>
</port>
</condition>
<condition name="cond">
 <port name="size">
<integer>2</integer>
</port>
 <port name="time-step">
<double>1.000000000000000</double>
</port>
</condition>
<condition name="cond_A">
 <port name="name">
<string>a</string>
</port>
</condition>
<condition name="cond_B">
 <port name="name">
<string>b</string>
</port>
</condition>
<condition name="cond_C">
 <port name="name">
<string>c</string>
</port>
</condition>
<condition name="cond_D">
 <port name="name">
<string>d</string>
</port>
</condition>
</conditions>
<views>
<outputs>
<output name="view" location="" package="vle.output" format="local" plugin="storage">
     <map>
      <key name="columns">
       <integer>15</integer>
      </key>
      <key name="header">
       <string>top</string>
      </key>
      <key name="inc_columns">
       <integer>10</integer>
      </key>
      <key name="inc_rows">
       <integer>10</integer>
