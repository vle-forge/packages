</port>

</observable>

<observable name="obsB">
<port name="b">
 <attachedview name="view"/>
</port>

</observable>

<observable name="obsC">
<port name="c">
 <attachedview name="view"/>
</port>

</observable>

<observable name="obsD">
<port name="d">
 <attachedview name="view"/>
</port>

</observable>

</observables>
<view name="view" timestep="1.000000000000000" output="view" type="timed"/>

</views>
</experiment>
</vle_project>
