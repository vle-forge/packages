<?xml version="1.0" encoding="UTF-8" ?>
<vle_project author="Eric Ramat" version="0.5" date="mar, 18 nov 2008">
 <structures>
<model name="m" height="367" width="721" type="coupled">
<submodels>
<model name="a" height="45" dynamics="dynA" width="100" type="atomic" observables="obsA" x="160" y="75" conditions="cond,cond_A">
<in>
 <port name="perturb"/>
</in>
<out>
 <port name="update"/>
</out>
</model>
<model name="b" height="45" dynamics="dynB" width="100" type="atomic" observables="obsB" x="328" y="126" conditions="cond,cond_B">
<in>
 <port name="update"/>
</in>
<out>
 <port name="update"/>
</out>
</model>
<model name="c" height="45" dynamics="dynC" width="100" type="atomic" observables="obsC" x="499" y="189" conditions="cond,cond_C">
<in>
 <port name="update"/>
</in>
<out>
 <port name="update"/>
</out>
</model>
<model name="d" height="45" dynamics="dynD" width="100" type="atomic" observables="obsD" x="563" y="282" conditions="cond,cond_D">
<in>
 <port name="update"/>
</in>
<out>
 <port name="update"/>
</out>
</model>
<model name="perturb" height="45" dynamics="dynPerturb" width="100" type="atomic" x="17" y="27">
<out>
